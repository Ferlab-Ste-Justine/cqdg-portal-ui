{
  "name": "cqdg-portal-ui",
  "version": "0.5.0",
  "private": true,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:netlify": "npm ci && npm run build",
    "test": "react-scripts test --coverage --coverageReporters=\"json-summary\"",
    "test:github": "react-scripts test --coverage --watchAll=false --no-watchman --coverageReporters=\"json-summary\"",
    "coverage": "npm test -- --watchAll=false --coverage --coverageReporters=\"json-summary\"",
    "coverage:changed": "npm test -- --coverage --watchAll=false --onlyChanged --coverageReporters=\"json-summary\"",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "theme": "mkdir -p src/style/themes/cqdg/dist && npm run theme:antd",
    "theme:watch": "nodemon --watch './src/style/themes' -e scss,less --exec 'npm run theme'",
    "theme:antd": "npm run theme:antd:colors && npm run theme:antd:main",
    "theme:antd:main": "lessc --js src/style/themes/cqdg/antd-cqdg-theme.less src/style/themes/cqdg/dist/antd.css",
    "theme:antd:colors": "less2sass ./src/style/themes/cqdg/colors.less && mv -f ./src/style/themes/cqdg/colors.scss ./src/style/themes/cqdg/dist/_colors.scss",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "commit:lint": "commitlint --edit $1",
    "update:config": "npx @ferbab/config",
    "cypress:install": "cypress install",
    "cypress:open": "cypress open",
    "cypress:run:cli": "cypress run",
    "cypress:run:chrome": "cypress run -b chrome"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "all",
    "singleQuote": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ferlab-Ste-Justine/cqdg-portal-ui"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint:fix"
    ],
    "!src/cypress/**/*.{ts,tsx}": "echo Ignoring Cypress files"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.1",
    "@apollo/client": "^3.7.11",
    "@dnd-kit/core": "^4.0.3",
    "@dnd-kit/sortable": "^5.1.0",
    "@ferlab/ui": "^9.9.7",
    "@loadable/component": "^5.15.2",
    "@nivo/bar": "^0.84.0",
    "@nivo/pie": "^0.84.0",
    "@react-keycloak/core": "^3.2.0",
    "@react-keycloak/web": "^3.4.0",
    "@reduxjs/toolkit": "^1.6.2",
    "antd": "^4.24.15",
    "antd-img-crop": "^4.12.2",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "compressorjs": "^1.2.1",
    "d3": "^5.16.0",
    "date-fns": "2.29.3",
    "file-saver": "^2.0.2",
    "graphql": "^16.2.0",
    "html-react-parser": "^1.4.4",
    "http-errors": "^1.7.2",
    "jwt-decode": "^3.1.2",
    "keycloak-js": "^21.1.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-highlight-words": "^0.20.0",
    "react-intl-universal": "^2.4.8",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.22.1",
    "react-scripts": "5.0.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-filter": "^0.0.20",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.14.5",
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/d3": "^7.1.0",
    "@types/file-saver": "^2.0.2",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^29.5.11",
    "@types/loadable__component": "^5.13.4",
    "@types/lodash": "^4.14.178",
    "@types/md5": "^2.3.1",
    "@types/node": "^16.11.7",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@types/react-highlight-words": "^0.16.4",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-logger": "^3.0.9",
    "@types/uuid": "^8.3.2",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "cra-bundle-analyzer": "^0.1.1",
    "cypress": "^10.11.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "husky": "^8.0.1",
    "less2sass": "^1.0.3",
    "lint-staged": "^13.0.3",
    "node-sass": "^6.0.1",
    "prettier": "^2.0.5",
    "prettier-eslint": "^10.1.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.2"
  }
}
